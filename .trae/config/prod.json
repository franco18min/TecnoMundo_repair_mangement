{
  "env": "prod",
  "tokens": {
    "maxMode": {
      "threshold": 24000,
      "warningAt": 20000,
      "hardLimit": 32000,
      "compression": {
        "minifyJson": true,
        "minifyCode": true,
        "removeComments": true,
        "stripWhitespace": true
      },
      "prioritization": {
        "segments": ["code", "paths", "commands", "schemas"],
        "dropLowValue": true
      }
    }
  },
  "monitoring": {
    "metricsFile": "cache/metrics.json",
    "logLevel": "error"
  }
}